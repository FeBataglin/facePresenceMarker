import { PoBreadcrumb } from '@po-ui/ng-components';
import { ActivatedRoute } from '@angular/router';
export declare class BreadcrumbControlService {
    breadcrumb: PoBreadcrumb;
    constructor();
    newBreadcrumb(): void;
    addBreadcrumb(literal: string, activatedRoute: ActivatedRoute): void;
    addBreadcrumbURL(literal: string, url: string): void;
    updBreadcrumbURL(literal: string, valueOld: string, valueNew: string): void;
    getBreadcrumb(): PoBreadcrumb;
    getCurrentRouter(): string;
    getPrevRouter(): string;
    hasPreviousRouter(): boolean;
}
